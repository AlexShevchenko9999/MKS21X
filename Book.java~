public class Book{
    String author;
    String title;
    int ISBN;

    public book(){};
    
    public book(author, title, ISBN){
	this.author = author;
	this.title = title;
	this.ISBN = ISBN;
    }

    public void set();
    
    public String get();
    public int get();

    public String toString(){
	
	
    

}